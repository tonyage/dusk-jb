<idea-plugin>
  <id>com.tonyage.dusk</id>
  <name>Dusk Theme</name>
  <version>@version@</version>
  <vendor email="tonyttdo@gmail"
          url="https://github.com/tonyage/dusk-theme">Tony Do
  </vendor>

  <depends>com.intellij.modules.platform</depends>

  <!--    Leave this in here so the IDE gives you feedback on-->
  <!--    What API's are available-->
  <idea-version since-build="203.7148.57"/>

  <extensions defaultExtensionNs="com.intellij">
    <applicationService serviceImplementation="com.tonyage.settings.ThemeSettings"/>
    <notificationGroup displayType="BALLOON" id="Dusk Theme" icon="Icons.LOGO" isLogByDefault="false" toolWindowId="dusk"  />
    <postStartupActivity implementation="com.tonyage.DuskTheme"/>
    <themeProvider id="f92a0fa7-1a98-47cd-b5cb-78ff67e6f4f3" path="/themes/dusk.theme.json"/>
    <themeProvider id="1a92aa6f-c2f1-4994-ae01-6a78e43eeb24" path="/themes/dusk_italic.theme.json"/>
    <themeProvider id="4b6007f7-b596-4ee2-96f9-968d3d3eb392" path="/themes/dusk_vivid.theme.json"/>
    <themeProvider id="4f556d32-83cb-4b8b-9932-c4eccc4ce3af" path="/themes/dusk_vivid_italic.theme.json"/>
  </extensions>
</idea-plugin>
